{
  "name": "youtube-search-tester",
  "version": "1.0.0",
  "description": "Simple YouTube search automation using Puppeteer",
  "main": "youtube-search.js",
  "scripts": {
    "start": "node youtube-search.js",
    "server": "node http-server.js",
    "test": "node youtube-search.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:check": "retire --path .",
    "security:test": "snyk test",
    "security:monitor": "snyk monitor",
    "pre-commit": "npm run lint && npm run security:check && npm run security:test",
    "prepare": "husky install",
    "deploy": "vercel --prod",
    "dev": "vercel dev"
  },
  "dependencies": {
    "express": "^4.21.2",
    "puppeteer": "^24.20.0"
  },
  "keywords": [
    "puppeteer",
    "youtube",
    "automation",
    "web-scraping"
  ],
  "author": "YouTube Search Tester",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "retire": "^5.3.0"
  }
}
